<?xml version="1.0"?>
<object name="portal_javascripts">
  <!-- jquery tools dateinput, from plone.app.jquerytools, which does not
       enable this resource by default, so we do it here -->

  <javascript
    cacheable="True"
    compression="none"
    cookable="True"
    enabled="True"
    id="++resource++plone.app.jquerytools.validator.js"
    inline="False"
    expression=""
    insert-after="++resource++plone.app.jquerytools.js"
    />
  
  <javascript
    cacheable="True"
    compression="none"
    cookable="True"
    enabled="True"
    id="++resource++uu.formlibrary.js/moment.min.js"
    inline="False"
    expression=""
    />

  <javascript
    cacheable="True"
    compression="none"
    cookable="True"
    enabled="True"
    id="++resource++uu.formlibrary.js/type-a-date-pattern.js"
    inline="False"
    expression=""
    insert-after="++resource++uu.formlibrary.js/moment.min.js"
    />

</object>
